<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean name="student3"  class="com.example.Springassignment1.task3.Student">
        <constructor-arg name = "id" value ="1"/>
        <constructor-arg name = "name" value = "Kumar Yadav"/>
        <constructor-arg name = "address"  ref="address3"/>
    </bean>

    <bean name="address3" class="com.example.Springassignment1.task3.Address">
        <property name="address" value="Pokhara"/>
    </bean>
</beans>